# Compute Fibonacci Numbers

print "Fibonacci Numbers" nl nl

set count = 0
set prevfibo = 1
set currfibo = 0
while count <= 68
    print "F("
    print count
    print ") = "
    print currfibo nl

    set nextfibo = prevfibo + currfibo
    set prevfibo = currfibo
    set currfibo = nextfibo
    set count = count+1
end

